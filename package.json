{
    "name": "i-cant-believe-gits-not-a-file",
    "displayName": "I can't believe git's not a file",
    "description": "Access and edit the git index as if it's a real filesystem",
    "version": "0.0.1",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ErnWong/i-cant-believe-gits-not-a-file"
    },
	"engines": {
		"vscode": "^1.100.0"
	},
	"categories": [
		"SCM Providers"
	],
	"activationEvents": [
		"onFileSystem:icantbelievegit"
	],
	"extensionDependencies": [
		"vscode.git"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "icantbelievegit.openStagedForCurrentFile",
				"title": "Open Staged Version of Current File (Editable)",
				"category": "Git"
			},
			{
				"command": "icantbelievegit.openStagedForPath",
				"title": "Open Staged File (Editable)",
				"category": "Git"
			},
			{
				"command": "icantbelievegit.openStagedChanges",
				"title": "Open Staged Changes (Editable)",
				"category": "Git"
			},
			{
				"command": "icantbelievegit.diffCurrentFile",
				"title": "Diff Current File (Editable)",
				"category": "Git"
			}
		],
		"menus": {
			"commandPalette": [
				{
				    "command": "icantbelievegit.openStagedForCurrentFile"
				},
				{
				    "command": "icantbelievegit.openStagedForPath"
				},
				{
				    "command": "icantbelievegit.openStagedChanges"
				},
				{
				    "command": "icantbelievegit.diffCurrentFile"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^22",
		"@types/vscode": "^1.100.0",
		"typescript": "^5.9.2"
	}
}